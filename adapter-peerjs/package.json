{
  "name": "@pulsebeam/adapter-peerjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "author": "Gabrielle Herman support@pulsebeam.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PulseBeamDev/pulsebeam-js.git"
  },
  "homepage": "https://pulsebeam.dev",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.mts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@pulsebeam/peer": "^0.0.17"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.6",
    "vitest": "^3.0.6",
    "tsup": "^8.3.6"
  },
  "tsup": {
    "entry": [
      "lib/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "minify": true,
    "sourcemap": true,
    "target": "esnext",
    "outDir": "dist"
  }
}
