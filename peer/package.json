{
  "name": "@pulsebeam/peer",
  "version": "0.0.22",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "gen": "bash ./generate.bash",
    "dev": "vite",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "clean": "rm -rf dist",
    "test": "vitest",
    "preview": "vite preview",
    "show:docs": "npx deno doc --html index.ts && python -m http.server -d docs/",
    "publish": "jsr publish --allow-slow-types"
  },
  "keywords": [
    "typescript",
    "library",
    "browser"
  ],
  "author": "Lukas Herman support@pulsebeam.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PulseBeamDev/pulsebeam-js.git"
  },
  "homepage": "https://pulsebeam.dev",
  "license": "Apache-2.0",
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.9.5",
    "@protobuf-ts/runtime": "^2.9.4",
    "@protobuf-ts/runtime-rpc": "^2.9.4",
    "jwt-decode": "^4.0.0",
    "nanostores": "^0.11.4"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "jsr": "^0.13.2",
    "protoc-gen-ts": "^0.8.7",
    "tsup": "^8.4.0",
    "typescript": "^5.7.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.5",
    "webrtc-adapter": "^9.0.1"
  }
}
