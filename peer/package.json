{
  "name": "@pulsebeam/peer",
  "version": "0.0.11",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "gen": "protoc --ts_out=. --experimental_allow_proto3_optional --ts_opt=client_generic --proto_path proto/v1 ./proto/v1/tunnel.proto && node ./scripts/patch.cjs",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "publish": "jsr publish --allow-slow-types"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "library",
    "browser"
  ],
  "author": "Lukas Herman support@pulsebeam.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PulseBeamDev/pulsebeam-js.git"
  },
  "homepage": "https://pulsebeam.dev",
  "license": "Apache-2.0",
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.4",
    "@protobuf-ts/runtime-rpc": "^2.9.4",
    "@protobuf-ts/twirp-transport": "^2.9.4",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "jsr": "^0.13.2",
    "protoc-gen-ts": "^0.8.7",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "webrtc-adapter": "^9.0.1"
  },
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "minify": true,
    "sourcemap": true,
    "target": "esnext",
    "outDir": "dist"
  }
}
